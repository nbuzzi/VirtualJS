@{
    Layout = null;
}
<style>
    td {
        color: black !important;
    }

    th {
        color: white !important;
    }

    table {
        background-color: white !important;
    }

    .thead-inverse {
        background-color: black !important;
    }
</style>

<div>
    <h1>{{pepe}}</h1>
    <h2>{{counter}}</h2>
    <button class="my-button btn btn-primary btn-sm">Increment</button>
    <a href="/" class="btn btn-warning btn-sm">Back to Home</a>
</div>

<div style="margin-top: 2%;" class="panel">
    <table class="table">
        <thead class="thead-inverse">
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Ages
                </th>
            </tr>
        </thead>
        <tbody>
            <tr vdom-repeat="item in list">
                <td>{{item.name}}</td>
                <td>{{item.age}}</td>
            </tr>
        </tbody>
    </table>
</div>

<div style="margin-top: 2%;" class="panel">
    <table class="table">
        <thead class="thead-inverse">
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Name
                </th>
            </tr>
        </thead>
        <tbody>
            <tr vdom-repeat='item in users'>
                <td>{{item.id}}</td>
                <td>{{item.name}}</td>
            </tr>
        </tbody>
    </table>
</div>

<div vdom-if="counter < 10">
    El contador se encuentra actualmente en un numero menor a 10, actual {{counter}}/10
</div>

<div vdom-if="counter >= 10">
    Se supero el contador, numero actual {{counter}}
</div>

<div style="margin-top: 2%;" vdom-partial="/Home/Other">

</div>